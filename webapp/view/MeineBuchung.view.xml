<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:table="sap.ui.table" controllerName="raumreservierung.controller.MeineBuchung" displayBlock="true">
    <Page showHeader="false" id="page" 
    >
        <mvc:XMLView viewName="raumreservierung.view.NavBar" class="navBarB" />


        <content>
            <table:Table id="myTable" class="sapUiResponsiveMargin" visibleRowCount="10" width="" selectionMode="MultiToggle" >
        

                <table:extension>
                    <HBox>
                        <items>
                            <Title text="Meine Buchungen" titleStyle="H3" class="sapUiTinyMarginBeginEnd"/>
                        </items>

                        <FacetFilter id="filter" showReset="false">
                            <FacetFilterList title="Raumnummer" listClose="onFilter">
                                <FacetFilterItem text="1" key="1"/>
                                <FacetFilterItem text="2" key="2"/>
                                <FacetFilterItem text="3" key="3"/>
                            </FacetFilterList>
                        </FacetFilter>
                    </HBox>
                </table:extension>
        
            </table:Table>
        </content>

        <footer>
            <OverflowToolbar>
                <ToolbarSpacer/>
                <Button text="Stornieren" class='sapUiMediumMargin' press="onCancelBooking" type='Reject'/>
            </OverflowToolbar>
        </footer>

        
    </Page>
</mvc:View>
