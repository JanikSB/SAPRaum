<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" 
xmlns:mvc="sap.ui.core.mvc" 
xmlns="sap.m" 
xmlns:layout= "sap.ui.layout"
controllerName="raumreservierung.controller.NavBar" 
displayBlock="true"
xmlns:tnt="sap.tnt"> 
     <tnt:ToolPage id="toolPage">

		<tnt:subHeader>
				<IconTabBar headerMode="Inline" select=".onSelectWindow" class="sapUiResponsiveContentPadding" expandable="false" expanded="false">
						<items>
							<IconTabFilter key="raumbuchung" text="Raumbuchung" />
							<IconTabFilter key="buchungen" text="Meine Buchungen" />
							<IconTabFilter key="profil" text="Mein Profil" />

							<IconTabSeparator />

							<IconTabFilter key='logout' text="Sign Out" icon='sap-icon://log' class="sapUiLargeMargin"/>
						</items>
				</IconTabBar>
		</tnt:subHeader>
        
    </tnt:ToolPage>

	

    
    
</mvc:View>